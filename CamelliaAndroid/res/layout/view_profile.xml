<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent" android:background="@color/search_results_stats_background">

	<include layout="@layout/header_row" />
	<include layout="@layout/view_profile_nav" />

	<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:background="@color/white"
		android:orientation="horizontal">
		<ImageView android:id="@+id/view_profile_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/profile_icon"></ImageView>

		<LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:orientation="vertical">
			<TextView android:id="@+id/view_profile_name" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge"
				android:text="TextView" android:textColor="@color/black"></TextView>
			<TextView android:id="@+id/view_profile_description" android:layout_width="wrap_content" android:text="name here" android:layout_height="wrap_content"
				android:textAppearance="?android:attr/textAppearanceLarge" android:textColor="@color/black"></TextView>
			<TextView android:id="@+id/view_profile_vanity_url" android:layout_width="wrap_content" android:text="http://www.url.com" android:layout_height="wrap_content"
				android:textAppearance="?android:attr/textAppearanceMedium" android:autoLink="email" android:textColor="@color/blue"></TextView>
		</LinearLayout>
	</LinearLayout>

	<LinearLayout android:id="@+id/view_profile_home_section" android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="vertical">
		<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/contact_point_icon_and_value" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:orientation="horizontal">
			<ImageView android:layout_width="wrap_content" android:src="@drawable/icon_lock" android:layout_height="wrap_content" android:id="@+id/imageView1"></ImageView>
			<TextView android:id="@+id/view_profile_home_address" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge"
				android:text="TextView" android:textColor="@color/black"></TextView>
		</LinearLayout>
		
		<TextView android:id="@+id/view_profile_home_mobile" android:layout_width="wrap_content" android:text="name here" android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceLarge" android:textColor="@color/black"></TextView>
		<TextView android:id="@+id/view_profile_home_telephone" android:layout_width="wrap_content" android:text="http://www.url.com" android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium" android:textColor="@color/blue"></TextView>
	</LinearLayout>
</LinearLayout> 